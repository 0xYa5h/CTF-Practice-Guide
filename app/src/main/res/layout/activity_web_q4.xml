<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    tools:context=".web_q1">

    <TextView
        android:id="@+id/head_wq4"
        android:layout_width="234dp"
        android:layout_height="55dp"
        android:text="Prehashbrown"
        android:textAlignment="textStart"
        android:textColor="@android:color/background_dark"
        android:textSize="30sp"
        android:textStyle="bold"
        android:enabled="true"
        android:textIsSelectable="true"
        android:longClickable="true"
        android:focusable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.002" />

    <TextView
        android:id="@+id/chal_wq4"
        android:layout_width="389dp"
        android:layout_height="199dp"
        android:text="I created a database of all known types of hashbrowns! Try to see if you can find a way to authenticate as an admin and retrieve the flag.http://138.197.193.132:5000/login"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:enabled="true"
        android:textIsSelectable="true"
        android:longClickable="true"
        android:focusable="true"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/head_wq4"
        app:layout_constraintVertical_bias="0.199" />

    <TextView
        android:id="@+id/point_wq4"
        android:layout_width="92dp"
        android:layout_height="40dp"
        android:text="10 Points"
        android:textAlignment="textStart"
        android:textColor="@android:color/background_dark"
        android:textSize="18sp"
        android:textStyle="bold"
        android:enabled="true"
        android:textIsSelectable="true"
        android:longClickable="true"
        android:focusable="true"
        app:layout_constraintBottom_toTopOf="@+id/chal_wq4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.84"
        app:layout_constraintStart_toEndOf="@+id/head_wq4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037" />

    <EditText
        android:id="@+id/ans_wq4"
        android:layout_width="383dp"
        android:layout_height="202dp"
        android:layout_marginBottom="120dp"
        android:ems="10"
        android:hint="Answer"
        android:textIsSelectable="true"
        android:longClickable="true"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.448"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/head_wq4"
        app:layout_constraintVertical_bias="0.861" />

    <Button
        android:id="@+id/sub_wq4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00ACC1"
        android:text="Submit"
        android:textColor="@android:color/background_dark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.915"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ans_wq4"
        app:layout_constraintVertical_bias="0.478" />


</androidx.constraintlayout.widget.ConstraintLayout>